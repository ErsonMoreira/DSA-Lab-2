bool searchIterative(BSTNode* root, int key) {
    if (root == nullptr) { return false; }

    BSTNode* cur = root;
    while (cur) {
        if(key == cur->data) {
            return true;
        }
        if (key < cur->data) { cur = cur->left;} 
        else { cur = cur->right; }
    }
    return false;
}
